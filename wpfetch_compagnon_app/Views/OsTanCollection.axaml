<TemplatedControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="wpfetch_compagnon_app.OsTanCollection"
			 xmlns:vm="clr-namespace:wpfetch_compagnon_app.ViewModels"
			 x:DataType="vm:OsTanCollectionViewModel">

	<StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="20">

		<Button Content="⬅️" Command="{Binding SetBackwardButtonCommand}"/>

		<Image Source="{Binding OsTanImg}" MaxWidth="200" MaxHeight="200"/>
		<TextBlock Text="{Binding OsTanDesc}"/>

		<Button Content="➡️" Command="{Binding SetForwardButtonCommand}"/>

	</StackPanel>
</TemplatedControl>
	
